package javastart.b01_object;


class Man{
	
	String name;
	int age;
	void printAll() {
		System.out.print("이름은"+name);
		System.out.println(",나이는"+age+"살!!");
		
	}
}

public class A03_ObjMethod {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Man m1 = new Man();
		m1.name = "이경규";
		m1.age = 52;
		m1.printAll();
		
		
		
	}//main

}
